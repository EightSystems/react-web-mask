{
  "name": "react-web-mask",
  "version": "1.0.0",
  "description": "Masked inputs for web React applications.",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,json}\"",
    "prebuild": "rm -rf dist/",
    "build": "yarn prebuild && rollup -c",
    "publish:local": "yarn build && yalc publish --files --no-scripts",
    "publish:npm": "yarn semantic-release"
  },
  "repository": {
    "type": "git"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,html,md,yml}": [
      "yarn format"
    ]
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1"
  },
  "dependencies": {
    "tinymask": "1.0.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-destructuring": "^7.14.7",
    "@babel/plugin-transform-parameters": "^7.15.4",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.56.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "semantic-release": "^17.4.7"
  }
}
